  app.get("/api/surgical-approaches/:id/complete", isAuthenticated, async (req: Request, res: Response) => {
    try {
      const approachId = parseInt(req.params.id);
      const primaryCidId = req.query.primaryCidId ? parseInt(req.query.primaryCidId as string) : null;

