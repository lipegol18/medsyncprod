        })
        .from(surgicalApproachOpmeItems)
        .innerJoin(opmeItems, eq(surgicalApproachOpmeItems.opmeItemId, opmeItems.id))
        .where(opmeWhereCondition)
        .orderBy(surgicalApproachOpmeItems.isRequired);

      // Buscar fornecedores associados
      // Se primaryCidId for fornecido, buscar apenas associa√ß√µes espec√≠ficas para esse CID
      const supplierWhereCondition = primaryCidId 
        ? and(
