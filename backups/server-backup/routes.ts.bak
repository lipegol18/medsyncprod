      console.log(`‚úÖ Dados completos encontrados: ${associatedProcedures.length} procedimentos, ${associatedOpmeItems.length} OPME, ${suppliersData.length} fornecedores, ${justifications.length} justificativas, ${additionalCids.length} CIDs autom√°ticos`);

      res.json(completeData);
    } catch (error) {
      console.error("Erro ao buscar dados completos da conduta cir√∫rgica:", error);
