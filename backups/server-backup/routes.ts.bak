    async (req: Request, res: Response) => {
      try {
        const orderId = parseInt(req.params.orderId);
        
        if (!orderId) {
          return res.status(400).json({ message: "ID do pedido m√©dico √© obrigat√≥rio" });
        }

        console.log(`Buscando procedimentos CBHPM para pedido ${orderId}`);

