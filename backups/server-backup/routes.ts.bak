          updateData.password = await bcrypt.hash(req.body.password, 10);
        }
        
        console.log(`Atualizando usu√°rio ${userId} com dados:`, {
          ...updateData,
          password: updateData.password ? "[REDACTED]" : undefined
        });
        
        // Atualizar o usu√°rio
